{
  "description": "A Redux base for you to build graphics on when using Twitchie!",
  "keywords": [
    "nodecg-bundle"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloitsdan/nodecg-twitchie-graphics.git"
  },
  "homepage": "https://github.com/helloitsdan/nodecg-twitchie-graphics#readme",
  "name": "nodecg-twitchie-graphics",
  "version": "2.4.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "dani quinn sexton",
    "email": "dani@glitter.tech",
    "url": "http://glitter.tech"
  },
  "scripts": {
    "build": "npm run build:dashboard && npm run build:client",
    "build:client": "rollup --config rollup.config.client.js",
    "build:dashboard": "webpack --config webpack.config.dashboard.js",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "src/client/**/*.ts": "tslint"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloitsdan/nodecg-twitchie-graphics/issues"
  },
  "peerDependencies": {
    "nodecg-twitchie": "^2.4.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "parcel": "^2.0.0-alpha.3.2",
    "prettier": "^1.19.1",
    "rollup": "^2.0.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "files": [
    "dashboard",
    "extension",
    "graphics",
    "lib"
  ],
  "nodecg": {
    "compatibleRange": ">=1.5.0",
    "dashboardPanels": [
      {
        "name": "brb",
        "title": "BRB!",
        "width": 2,
        "file": "brb.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "timer",
        "title": "Timer!",
        "width": 2,
        "file": "timer.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "social",
        "title": "Social Accounts",
        "width": 2,
        "file": "social.html",
        "headerColor": "#6441A4",
        "workspace": "Config"
      }
    ]
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "@polymer/iron-flex-layout": "^3.0.0",
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-list": "^3.1.0",
    "@polymer/iron-pages": "^3.0.0",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/paper-checkbox": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.0",
    "@polymer/polymer": "^3.3.1",
    "moment": "^2.24.0",
    "nodecg-twitchie": "^2.4.0",
    "redux": "^4.0.5"
  }
}
