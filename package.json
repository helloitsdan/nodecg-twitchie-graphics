{
  "name": "nodecg-twitchie-graphics",
  "version": "2.2.0",
  "description": "A Redux base for you to build graphics on when using Twitchie!",
  "main": "lib/index.js",
  "author": {
    "name": "dani quinn sexton",
    "email": "dani@glitter.tech",
    "url": "http://glitter.tech"
  },
  "scripts": {
    "build": "npm run build:client",
    "build:client": "rollup -c",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "{overlay-provider,twitchie}/**/*.{js,jsx}": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloitsdan/nodecg-twitchie-graphics.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloitsdan/nodecg-twitchie-graphics/issues"
  },
  "homepage": "https://github.com/helloitsdan/nodecg-twitchie-graphics#readme",
  "peerDependencies": {
    "nodecg-twitchie": "2.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "rollup": "^2.0.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dashboard",
    "extension",
    "graphics",
    "lib"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": ">=1.5.0",
    "dashboardPanels": [
      {
        "name": "brb",
        "title": "BRB!",
        "width": 2,
        "file": "brb.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "timer",
        "title": "Timer!",
        "width": 2,
        "file": "timer.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "social",
        "title": "Social Accounts",
        "width": 2,
        "file": "social-media.html",
        "headerColor": "#6441A4",
        "workspace": "Config"
      }
    ]
  }
}
